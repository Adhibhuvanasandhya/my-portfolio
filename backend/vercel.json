{
  "version": 2,
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/send",
          "methods": ["POST", "OPTIONS"],
          "dest": "/index.js"
      },
      {
          "src": "/(.*)",
          "dest": "/index.js"
      }
  ]
}
